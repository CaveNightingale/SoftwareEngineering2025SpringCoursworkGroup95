<?xml version="1.0" encoding="UTF-8"?>

<?import io.github.software.coursework.gui.EntityItemController?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Text?>
<AnchorPane
        xmlns="http://javafx.com/javafx"
        xmlns:fx="http://javafx.com/fxml"
        fx:controller="io.github.software.coursework.gui.EntityItemController"
>
    <AnchorPane
            styleClass="entity-item"
            AnchorPane.leftAnchor="0.0"
            AnchorPane.rightAnchor="0.0"
    >
        <VBox
                styleClass="entity-item-content"
                AnchorPane.leftAnchor="0.0"
                AnchorPane.rightAnchor="0.0"
        >
            <HBox styleClass="entity-item-1st-row">
                <Text fx:id="name" text="Jia Peidong"/>
            </HBox>
        </VBox>
    </AnchorPane>
</AnchorPane>
